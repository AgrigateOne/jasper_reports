	<parameter name="load_id" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[WITH trading_partner AS(
	SELECT fn_party_role_name(loads.exporter_party_role_id) AS exporter,
UPPER(addresses.address_line_1) AS address_line_1,
UPPER(addresses.address_line_2) AS address_line_2,
UPPER(addresses.address_line_3) AS address_line_3,
UPPER(addresses.city) AS city,
UPPER(addresses.postal_code) AS postal_code,
UPPER(addresses.country) AS country,
fn_party_contact_method(party_addresses.party_id, 'Tel') AS tel_no_1,
fn_party_contact_method(party_addresses.party_id, 'Fax') AS tel_no_2,
30 as trading_partner_int
FROM party_roles
LEFT JOIN organizations ON organizations.id = party_roles.organization_id
LEFT JOIN parties ON parties.id = party_roles.party_id
LEFT JOIN party_addresses ON party_addresses.party_id = party_roles.party_id
LEFT JOIN addresses ON addresses.id = party_addresses.address_id
  AND addresses.address_type_id = (SELECT id FROM address_types WHERE address_type = 'Delivery Address')
JOIN loads on loads.exporter_party_role_id = party_roles.id
WHERE loads.id = $P{load_id}
)
SELECT distinct vessels.vessel_code,
concat('42/' ,loads.exporter_certificate_code,'/',loads.id) as serial_no,
--array_to_string(array_agg(distinct(pucs.puc_code)), '/' ) AS puc,
commodities.code as commodity_code,
case mv.description
when 'CHEEKY ''™''' THEN 'CHEEKY™ (ARC PEAR 1)'
when 'CRIPPS'' RED' THEN 'CRIPPS’ RED'
when 'CRIPPS'' RED 60%' THEN 'CRIPPS’ RED'
when 'CRIPPS'' PINK' THEN 'CRIPPS’ PINK'
when 'CRIPPS'' PINK-PINK LADY©' THEN 'PINK LADY® (CRIPPS’ PINK)'
when 'CRIPPS PINK-PINK LADY©' then 'PINK LADY® (CRIPPS’ PINK)'
when 'TENROY-ROYAL GALA©' THEN 'ROYAL GALA® (TENROY)'
when 'CRIPPS'' RED-SUNDOWNER©' THEN 'SUNDOWNER® (CRIPPS’ RED)'
when 'FORELLE (VERMONT BEAUTY)' THEN 'FORELLE/VERMONT BEAUTY'
when 'CRIPPS'' RED-SUNDOWNER©' THEN 'SUNDOWNER® (CRIPPS’ RED)'
when 'BON CHRéTIEN' THEN 'WILLIAM’S BON CHRÉTIEN'
when 'FUJI' THEN 'FUJI'
when 'GREEN FUJI (FUGI)' THEN 'FUJI (GREEN FUJI)'
when 'RED FUJI' THEN 'FUJI (RED FUJI)'
when 'ROSEMARIE (SEMPRE)' then 'ROSEMARIE/SEMPRE'
ELSE mv.description END  as marketing_variety_description,
commodities.description as commodity_desc,
load_containers.container_code,
sum(ps.carton_quantity) as ctn_qty,
sum(ps.nett_weight) as nett_mass,
pod_ports.port_code as arrival_port_name,
case rmt_classes.description when 'CAT I' then 'CLASS 1' when 'CAT II' THEN 'CLASS 2' ELSE rmt_classes.description END as product_class_description,
destination_countries.description as country_medium_code,
tp.exporter as trading_partner_code,
tp.address_line_1||','|| tp.address_line_2||','||tp.city||','||tp.postal_code || ',' || tp.country as trader_name
from pallets p
JOIN pallet_sequences ps ON p.id = ps.pallet_id
LEFT JOIN loads ON loads.id = p.load_id
LEFT JOIN voyage_ports pod_voyage_ports ON pod_voyage_ports.id = loads.pod_voyage_port_id
LEFT JOIN ports pod_ports ON pod_ports.id = pod_voyage_ports.port_id
LEFT JOIN voyages ON voyages.id = pod_voyage_ports.voyage_id
LEFT JOIN vessels ON vessels.id = voyages.vessel_id
LEFT JOIN govt_inspection_pallets ON govt_inspection_pallets.id = p.last_govt_inspection_pallet_id
LEFT JOIN govt_inspection_sheets ON govt_inspection_sheets.id = govt_inspection_pallets.govt_inspection_sheet_id
JOIN pucs ON pucs.id = ps.puc_id
JOIN cultivar_groups ON cultivar_groups.id = ps.cultivar_group_id
LEFT JOIN commodities ON commodities.id = cultivar_groups.commodity_id
JOIN marketing_varieties mv ON mv.id = ps.marketing_variety_id
LEFT JOIN load_vehicles ON load_vehicles.id = loads.load_vehicle_id
LEFT JOIN load_containers ON load_containers.id = load_vehicles.load_container_id
LEFT JOIN rmt_classes ON rmt_classes.id = ps.rmt_class_id
LEFT JOIN destination_cities ON destination_cities.id = loads.final_destination_id
LEFT JOIN destination_countries ON destination_countries.id = destination_cities.destination_country_id
join  trading_partner tp on trading_partner_int = 30
where loads.id=$P{load_id}
group by
vessel_code,exporter_certificate_code,commodities.code,mv.description,commodities.description
,load_containers.container_code,pod_ports.port_code,rmt_classes.description,destination_countries.description
,tp.exporter,tp.address_line_1,tp.address_line_2,tp.city,tp.postal_code,tp.country,loads.id]]>
	</queryString>
