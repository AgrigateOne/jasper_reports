	<parameter name="mr_sales_return_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT sr.id, so.sales_order_number, so.erp_customer_number, org.short_description,
sum(quantity_returned)quantity_returned, soi.unit_price, soi.remarks, mrpv.product_variant_code, vt.percentage_applicable,
sum(quantity_returned * soi.unit_price) AS total_price,sum(src.amount) as costs
,(sum(quantity_returned * soi.unit_price)+sum(src.amount) )total_excluding_vat
,(sum(quantity_returned * soi.unit_price)+sum(src.amount) )*vt.percentage_applicable::dec(4,2)/100 as vat
,(sum(quantity_returned * soi.unit_price)+sum(src.amount) )+
((sum(quantity_returned * soi.unit_price)+sum(src.amount) )*vt.percentage_applicable::dec(4,2)/100) as total
FROM public.mr_sales_returns sr
LEFT OUTER JOIN mr_sales_return_items sri ON sri.mr_sales_return_id = sr.id
LEFT OUTER JOIN mr_sales_order_items soi ON sri.mr_sales_order_item_id = soi.id
LEFT OUTER JOIN mr_sales_orders so ON so.id = soi.mr_sales_order_id
LEFT OUTER JOIN material_resource_product_variants mrpv ON mrpv.id = soi.mr_product_variant_id
LEFT OUTER JOIN party_roles pr ON pr.id = so.customer_party_role_id JOIN organizations org ON org.id = pr.organization_id
LEFT OUTER JOIN mr_vat_types vt ON vt.id = so.vat_type_id
LEFT OUTER JOIN sales_return_costs src on src.mr_sales_return_id =sr.id
where sr.id= $P{mr_sales_return_id} 
GROUP BY sr.id, so.sales_order_number, so.erp_customer_number, org.short_description, soi.quantity_required, soi.unit_price, soi.remarks, mrpv.product_variant_code, vt.percentage_applicable]]>
	</queryString>
